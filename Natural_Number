/* Write a java Program to Print the Natural Number N Term using for loops*/

import java.io.*;
import java.util.*;

class looping {
   public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int Terms;
        System.out.print("Enter Terms : ");

        Terms = sc.nextInt();

       for (int i = 1; i < Terms; i++) {
        System.out.println(i+" ");
       }
   }
}
