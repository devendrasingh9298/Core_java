/* Write a java Program Using Data Member */
class Employee{
    int empid;
    String nm;
    static String com = "Some University";    
     Employee(int e, String n){
         empid = e;
         nm = n;
     }
     void Display(){
         System.out.println(empid+" "+nm+" "+com);
     }
    public static void main(String[] args) {
         Employee e1 = new Employee(101, "Alexa");
         e1.Display();
         Employee e2 = new Employee(102, "Boat");
         e2.Display();
         Employee e3 = new Employee(103, "Fire");
         e3.Display();
 }
}
