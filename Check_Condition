/*Check Condition between Three Number Using if...else*/
import java.io.*;
import java.util.*;

class condition {
   public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int firstNum,secondNum,thirdNum;
        System.out.print("Enter Three Numbers : ");

        firstNum = sc.nextInt();
        secondNum = sc.nextInt();
        thirdNum = sc.nextInt();

        if (firstNum > secondNum) {
            if (firstNum > thirdNum) {
                System.out.print("The Biggest of First Number : " + firstNum);
            }else{
                System.out.print("The Biggest of Third Number : "+ thirdNum);
        }
    }else{
        if(secondNum > thirdNum){
            System.out.print("The Biggest of Second Number : "+ secondNum);
        }else{
            System.out.print("The Biggest of Third Number : "+ thirdNum);
        }
    }
   }
}
